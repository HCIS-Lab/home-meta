{
    "name": "ROS 2 Humble Dev Container", // TODO
    "image": "lnfu/ros2:humble-desktop",
    "remoteUser": "ubuntu",
    "workspaceFolder": "/home/ubuntu/ros2_ws",
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/ubuntu/ros2_ws,type=bind",
    "runArgs": [
        "--network=host", // TODO
        "--gpus=all"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-python.python",
                "ms-python.black-formatter",
                "redhat.vscode-xml"
            ]
        }
    }
}
